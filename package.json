{
  "name": "vite-vue3-electron",
  "version": "0.0.0",
  "main": "dist/main/_.js",
  "scripts": {
    "start": "vite --open",
    "dev": "concurrently  -n=vue,ele -c=green,blue  \"yarn run dev:vue\" \"yarn run dev:ele \"",
    "dev:vue": "vite",
    "dev:ele": "node -r ts-node/register script/build --env=development --watch",
    "build": "yarn run build:vue && yarn run build:ele && electron-builder",
    "build:vue": "vue-tsc --noEmit && vite build",
    "build:ele": "node -r ts-node/register script/build --env=production",
    "serve": "vite preview"
  },
  "dependencies": {
    "@microsoft/signalr": "^5.0.5",
    "@types/js-cookie": "^2.2.6",
    "axios": "^0.21.1",
    "clipboard": "^2.0.8",
    "echarts": "^5.0.2",
    "element-plus": "^1.0.2-beta.36",
    "js-cookie": "^2.2.1",
    "js-md5": "^0.7.3",
    "moment": "^2.29.1",
    "path-to-regexp": "^6.2.0",
    "qier-progress": "^1.0.4",
    "qrcanvas": "^3.1.2",
    "throttle-debounce": "^3.0.1",
    "vue": "^3.0.5",
    "vue-json-editor": "^1.4.3",
    "vue-router": "4",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@types/minimist": "^1.2.1",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "chalk": "^4.1.0",
    "concurrently": "^6.0.0",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "electron-store": "^7.0.2",
    "minimist": "^1.2.5",
    "ora": "^5.4.0",
    "rollup-plugin-esbuild": "^3.0.2",
    "rollup-plugin-node-externals": "^2.2.0",
    "sass": "^1.32.8",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "vite": "^2.1.3",
    "vite-plugin-style-import": "^0.9.1",
    "vue-tsc": "^0.0.8"
  },
  "build": {
    "appId": "475832644@qq.com",
    "directories": {
      "output": "release/${version}"
    },
    "asar": false,
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  }
}
